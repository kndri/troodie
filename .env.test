# E2E Test Environment Variables
# Copy this to .env.test.local and fill in your values

# Supabase Test Instance
SUPABASE_TEST_URL=your_test_supabase_url
SUPABASE_TEST_ANON_KEY=your_test_anon_key
SUPABASE_SERVICE_KEY=your_service_key

# Test Accounts
TEST_EMAIL=test@troodie.com
TEST_PASSWORD=TestPassword123!
TEST_USERNAME=testuser

# Expo
EXPO_USERNAME=your_expo_username
EXPO_PASSWORD=your_expo_password

# App Configuration
APP_ENV=test
ENABLE_TEST_MODE=true

# Test Data
SEED_TEST_DATA=true
CLEANUP_AFTER_TESTS=true

# Maestro Configuration
MAESTRO_TIMEOUT=60000
MAESTRO_SCREENSHOT_ON_FAILURE=true